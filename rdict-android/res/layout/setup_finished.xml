<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget28"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout android:id="@+id/widget29"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:id="@+id/setup_finished_mesg" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Setup is done.  You can use RDict."
			android:layout_centerInParent="true"
			android:layout_marginLeft="50px"
			android:layout_marginRight="50px">
		</TextView>
	</RelativeLayout>
	
	<RelativeLayout android:id="@+id/widget31"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:id="@+id/setup_finished_button" android:layout_width="80px"
			android:layout_height="wrap_content" android:text="Finish"
			android:layout_centerInParent="true"
			android:layout_marginLeft="50px"
			android:layout_marginRight="50px">
		</Button>
	</RelativeLayout>
</LinearLayout>